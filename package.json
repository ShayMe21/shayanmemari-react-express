{
  "name": "shayanmemari-react-expres",
  "version": "1.0.0",
  "description": "Shayan Memari personal site with React/Express",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "serve": "npm run build && node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "make_copy": "rm -rf ~/git/react-node-boilerplate-for-interviews-master; mkdir ~/git/react-node-boilerplate-for-interviews-master ; cp -r ~/git/boilerplate_bak ~/git/react-node-boilerplate-for-interviews-master ; cd ~/git/react-node-boilerplate-for-interviews-master/boilerplate_bak ; npm install"
  },
  "author": "Shayan Memari",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "express": "^4.17.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.0.0",
    "css-loader": "^2.0.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^3.0.0",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.2",
    "prettier": "1.19.1",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3"
  }
}
